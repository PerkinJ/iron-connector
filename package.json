{
  "name": "icon-connector",
  "version": "0.0.1-beta-1",
  "description": "Lightweight multi-instance redux library based on react-redux",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+git@github.com:PerkinJ/iron-connector.git"
  },
  "keywords": [
    "hooks",
    "state"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PerkinJ/iron-connector/issues"
  },
  "homepage": "https://github.com/PerkinJ/iron-connector",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "lint": "npm run lint:nofix -- --fix",
    "lint:nofix": "eslint --cache --ext .ts,.tsx ./",
    "test": "NODE_ENV=unittest jest",
    "coverage": "codecov"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "@types/react-redux": "^7.1.1",
    "codecov": "^3.3.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.2.0",
    "jest": "^24.7.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-testing-library": "^7.0.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.7.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "preset": "ts-jest"
  }
}
